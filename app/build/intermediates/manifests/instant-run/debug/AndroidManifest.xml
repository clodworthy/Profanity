<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="profanity.mofogames.org"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="profanity.mofogames.org.ProfaneActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="profanity.mofogames.org.About"
            android:label="@string/about_title"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="profanity.mofogames.org.Preferences"
            android:label="@string/preferences_label" >
        </activity>
        <activity
            android:name="profanity.mofogames.org.DictionaryListView"
            android:label="@string/dictionary_label" >
        </activity>
        <activity
            android:name="profanity.mofogames.org.DictionaryActivity"
            android:label="@string/dictionary_label" >
        </activity>

        <service android:name="profanity.mofogames.org.AcceleromterService" />

        <receiver android:name="profanity.mofogames.org.BootBroadcastReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
    </application>

</manifest>